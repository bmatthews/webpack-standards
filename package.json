  "main": "index.js",
  "scripts": {
    "start": "babel-node --presets es2015,stage-0,react server.js",
    "push": "npm run build && npm run copy && npm run publish",
    "build": "cross-env NODE_ENV=production babel ./src --ignore ./src/__tests__ --out-dir .",
    "copy": "cp ./src/lint . -r",
